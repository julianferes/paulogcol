<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Mapper.DispositivoVO">
	
	
	<update id="idActualizarEstadoDispositivo" parameterType="DispositivoVO" >
	  UPDATE ma_dispositivo
	  SET ESTADO = 'R'
	  WHERE ID = #{id}
	</update>
	
</mapper>